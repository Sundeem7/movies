<h1>All Production Companies</h1>

<a href="/add">New Production Company</a>

<ul>
    {{!-- this will create an li for EACH item --}}
    {{#each allProductions}}
    <li>
        {{!-- print out name of production company you are looking for  --}}
        <a href="/{{_id}}">{{name}}</a>
        <ul>
            <li>Total Revenue: ${{revenue}}.00</li>
            <li>Amusement Park: {{amusementPark}}</li>
        </ul>
    </li>
    {{/each}}
</ul>