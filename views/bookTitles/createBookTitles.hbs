<form method="POST" action="/books">
<h1>Enter a New Book</h1>

<!-- Template for creating input form for a string object key called stringMember-->
    <label for="name">Book Title</label>
    <input type="text" name="name" required/>

    <label for="revenue">Revenue</label>
    <input type="number"  name="revenue" required/>

    <label for="amusementPark">Amusement Park</label>
    <select name="amusementPark">
        <option value="yes">Yes</option>
        <option value="no" selected>No</option>
    </select>

    <input type="submit" value="Create Book" />
</form>

