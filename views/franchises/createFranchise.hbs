<form method="POST" action="/series">
<h1>Enter A Franchise</h1>

<!-- Template for creating input form for a string object key called stringMember-->
    <label for="franchiseName">Franchise Name</label>
    <input type="text" name="franchiseName"/>

    <label for="numberOfMovies">Number of Movies in Franchise</label>
    <input type="number"  name="numberOfMovies"/>

    <label for="totalRevenue">Total Revenue</label>
    <input type="number"  name="totalRevenue"/>

    <label for="totalBudget">Total Budget</label>
    <input type="number"  name="totalBudget"/>

    <input type="submit" value="Add Franchise" />
</form>

